{
    "name": "acl-2024-schedulo",
    "version": "1.0.0",
    "description": "",
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write .",
        "dev": "concurrently \"npm run dev --prefix frontend --if-present\" \"npm run dev --prefix backend --if-present\"",
        "build": "npm run build --workspaces --if-present",
        "test": "npm run test --workspaces --if-present",
        "prestart": "npm run build",
        "start": "npm run start --workspaces --if-present"
    },
    "keywords": [],
    "author": "Schedulos",
    "type": "module",
    "license": "ISC",
    "devDependencies": {
        "@types/ejs": "^3.1.5",
        "concurrently": "^7.0.0",
        "dedupe": "^4.0.3",
        "ejs": "^3.1.10",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-html": "^8.1.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.5.0",
        "eslint-plugin-n": "^17.14.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.2.0",
        "eslint-plugin-security": "^3.0.1",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "jsdoc": "^4.0.4",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.8"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,css,md}": [
            "prettier --write"
        ]
    }
}
